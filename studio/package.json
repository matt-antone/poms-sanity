{
  "name": "starter-template",
  "private": true,
  "version": "1.0.0",
  "main": "package.json",
  "license": "UNLICENSED",
  "scripts": {
    "dev": "sanity dev",
    "start": "sanity start",
    "prebuild": "npm run extract-types",
    "build": "sanity build",
    "deploy": "sanity deploy",
    "extract-types": "sanity schema extract --enforce-required-fields",
    "upgrade": "npm install sanity@latest @sanity/vision@latest",
    "typegen": "sanity schema extract && sanity typegen generate --generates=../nextjs-app/sanity.types.ts"
  },
  "keywords": [
    "sanity"
  ],
  "overrides": {
    "react": "$react",
    "react-dom": "$react-dom"
  },
  "dependencies": {
    "@sanity/assist": "^3.2.2",
    "@sanity/icons": "^3.5.0",
    "@sanity/vision": "^3.88.3",
    "date-fns": "^3.6.0",
    "embla-carousel-autoplay": "^8.6.0",
    "motion": "^12.10.4",
    "next-sanity": "^9.12.0",
    "pluralize-esm": "^9.0.5",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "rxjs": "^7.8.1",
    "sanity": "^3.90.0",
    "sanity-plugin-asset-source-unsplash": "^3.0.3",
    "sanity-plugin-media": "^3.0.2",
    "sanity-plugin-simpler-color-input": "^3.0.1",
    "styled-components": "^6.1.13",
    "validator": "^13.15.0"
  },
  "devDependencies": {
    "@sanity/eslint-config-studio": "^4.0.0",
    "@types/react": "^18.3.12",
    "@types/validator": "^13.15.0",
    "eslint": "^8.6.0",
    "prettier": "^3.3.3",
    "typescript": "^5.6.3"
  },
  "prettier": {
    "semi": false,
    "printWidth": 100,
    "bracketSpacing": false,
    "singleQuote": true
  }
}
